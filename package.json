{
  "name": "ssh-keyman",
  "version": "2.0.0",
  "description": "A cli tool to manage multiple ssh keys and switch between different ssh keys with ease & grace.",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --maxWorkers=2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shahidullahkhankhattak/ssh-keyman.git"
  },
  "keywords": [
    "manage",
    "ssh",
    "keys",
    "ssh",
    "keys",
    "generate",
    "ssh",
    "github",
    "gitlab",
    "git",
    "ssh-keyman",
    "keyman",
    "key manager",
    "ssh key manager"
  ],
  "author": "shahid ullah khan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shahidullahkhankhattak/ssh-keyman/issues"
  },
  "homepage": "https://github.com/shahidullahkhankhattak/ssh-keyman#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^2.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "@types/jest": "^29.5.11"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!src/__tests__/helpers.js",
      "!src/__tests__/testUtils.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "helpers.js",
      "testUtils.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/__tests__/helpers.js",
      "/__tests__/testUtils.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 55,
        "lines": 55,
        "statements": 55
      }
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "coverage",
          "outputName": "junit.xml",
          "classNameTemplate": "{classname}",
          "titleTemplate": "{title}",
          "ancestorSeparator": " â€º ",
          "usePathForSuiteName": "true"
        }
      ]
    ]
  }
}
